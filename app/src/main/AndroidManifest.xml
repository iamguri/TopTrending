<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.toptrending">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TopTrending">
        <activity android:name=".TopTrendingActivity"/>
        <activity android:name=".BusinessActivity"/>
        <activity android:name=".ScienceActivity"/>
        <activity android:name=".BBCActivity"/>
        <activity android:name=".SportsActivity"/>
        <activity android:name=".TechnologyActivity"/>
        <activity android:name=".TimesOfIndiaActivity"/>
        <activity android:name=".NDTVActivity"/>
        <activity android:name=".FarmerActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>